<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to="login">
    <!-- GWT UiBinder and GwtBootstrap3 -->
    <inherits name="com.google.gwt.uibinder.UiBinder"/>
    <inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
    <inherits name="org.gwtbootstrap3.extras.bootbox.Bootbox"/>

    <!-- GWT-jQuery(gwtquery) -->
    <inherits name='com.google.gwt.query.Query'/>
    <inherits name='gwtquery.plugins.UiEmbedded'/>

    <!-- Entry Point -->
    <entry-point class="com.koobe.editor.login.client.LoginEntryPoint"/>

    <!-- Specify the paths for translatable code -->
    <source path='client'/>
    <source path='shared'/>

    <set-property name="compiler.stackMode" value="strip"/>

</module>