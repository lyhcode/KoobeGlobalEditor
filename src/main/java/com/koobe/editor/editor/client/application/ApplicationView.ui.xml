<!-- GWT BootStrap 3 -->
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:kui="urn:import:com.koobe.editor.common.client.ui">
    <ui:style>
    .footer {
        text-align: center;
        padding-top: 15px;
        padding-bottom: 25px;
    }
    </ui:style>

    <ui:with type="com.koobe.editor.editor.client.place.NameTokens" field="nameTokens"/>

    <b:Container>

        <g:HTMLPanel>
            <ul></ul>
        </g:HTMLPanel>

        <!-- Main Place Container -->
        <b:Container ui:field="container"/>

        <!-- Loading -->
        <kui:LoadingIndicator ui:field="loadingIndicator" />

    </b:Container>

</ui:UiBinder>